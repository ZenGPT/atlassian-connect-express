
{
    "development": {
        "port": 3000,
        "errorTemplate": true,
        "localBaseUrl": " https://test-app.public.atlastunnel.com",
        "store": {
            "adapter": "sequelize",
            "dialect": "sqlite3",
            "logging": false,
            "type": "memory"
        }
    },
    "production": {
        "port": "$PORT",
        "errorTemplate": true,
        "localBaseUrl": "https://my-subdomain.herokuapp.com",
        "store": {
            "url": "$DATABASE_URL"
        },
        "whitelist": [
            "*.jira-dev.com",
            "*.atlassian.net",
            "*.atlassian.com",
            "*.jira.com"
        ]
    },
    "product": "jira"
}